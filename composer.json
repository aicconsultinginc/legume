{
  "name": "kwhat/legume",
  "homepage": "https://github.com/kwhat/legume",
  "description": "Legume: Multi-thread Job Manager and Daemon",
  "keywords": ["daemon", "job", "manager", "beanstalk", "pheanstalk", "pthreads", "worker", "pool"],
  "type": "project",
  "license": "LGPL-3.0",
  "authors": [
    {
      "name": "Alexander Barker",
      "email": "alex@1stleg.com",
      "homepage": "https://github.com/kwhat"
    }
  ],
  "support": {
    "issues": "https://github.com/kwhat/legume/issues"
  },
  "minimum-stability": "stable",
  "require": {
    "php": "^5.6 || >=7.2",
    "monolog/monolog": "^1.22",
    "pda/pheanstalk": "~3.0",
    "pimple/pimple": "^3.0",
    "ulrichsg/getopt-php": "^3.0"
  },
  "suggest": {
    "ext-pthreads": "Required for thread support",
    "ext-pcntl": "Required for daemon support on Posix systems",
    "ext-posix": "Recommended for Posix systems"
  },
  "bin": [
    "bin/legumed"
  ],
  "autoload": {
    "psr-4": {
      "Legume\\": "src/"
    }
  }
}
